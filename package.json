    "@aws-sdk/lib-dynamodb": "^3.840.0",
    "@aws-sdk/signature-v4": "^3.370.0",
    "@pinecone-database/pinecone": "^6.1.2",
    "@types/gremlin": "^3.6.7",
    "@types/js-yaml": "^4.0.9",
    "axios": "^1.7.9",
    "gremlin": "^3.8.0",
    "js-yaml": "^4.1.1",
    "source-map-support": "^0.5.21",
    "uuid": "^9.0.0",
    "zod": "^3.25.76"
