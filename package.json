{
  "name": "cc-native",
  "version": "0.1.0",
  "description": "CC Native - AI-native autonomous revenue decision loop - policy-governed decision and execution system",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "watch": "tsc --watch",
    "lint": "eslint src/**/*.ts",
    "test": "npx jest --testPathIgnorePatterns=integration",
    "test:unit": "npx jest --testPathIgnorePatterns=integration",
    "test:coverage": "npx jest --testPathIgnorePatterns=integration --coverage",
    "test:integration": "npx jest --testPathPattern=tests/integration",
    "test:all": "npx jest",
    "clean": "rimraf dist",
    "cdk": "cdk",
    "cdk:build": "tsc",
    "cdk:watch": "tsc -w",
    "cdk:deploy": "cdk deploy",
    "cdk:destroy": "cdk destroy",
    "cdk:synth": "cdk synth",
    "cdk:diff": "cdk diff",
    "deploy": "npm run build && npm run cdk:deploy",
    "seed:methodologies": "ts-node src/scripts/seed-methodologies.ts",
    "seed:action-types": "ts-node src/scripts/seed-action-type-registry.ts",
    "seed:all": "ts-node src/scripts/seed-all.ts",
    "invoke:internal-adapter": "ts-node scripts/phase_4/invoke-internal-adapter.ts"
  },
  "keywords": [
    "autonomous",
    "revenue",
    "ai",
    "agentcore",
    "bedrock",
    "typescript",
    "aws",
    "cdk",
    "lambda",
    "decision-loop"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-crypto/sha256-js": "^5.2.0",
    "@aws-sdk/client-bedrock-agent-runtime": "^3.848.0",
    "@aws-sdk/client-bedrock-runtime": "^3.848.0",
    "@aws-sdk/client-cloudwatch": "^3.882.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.848.0",
    "@aws-sdk/client-dynamodb": "^3.840.0",
    "@aws-sdk/client-eventbridge": "^3.848.0",
    "@aws-sdk/client-iam": "^3.840.0",
    "@aws-sdk/client-kms": "^3.848.0",
    "@aws-sdk/client-lambda": "^3.840.0",
    "@aws-sdk/client-neptune": "^3.848.0",
    "@aws-sdk/client-qldb": "^3.848.0",
    "@aws-sdk/client-s3": "^3.883.0",
    "@aws-sdk/s3-request-presigner": "^3.883.0",
    "@aws-sdk/client-scheduler": "^3.840.0",
    "@aws-sdk/client-secrets-manager": "^3.848.0",
    "@aws-sdk/client-sfn": "^3.848.0",
